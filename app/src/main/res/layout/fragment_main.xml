                            android:layout_height="wrap_content"
                            android:id="@+id/dhuhr_check"
                            android:button="@drawable/mynotiftoggle_01"
                            android:background="@null"
                            android:layout_marginRight="5dp"
                            android:checked="false"/>

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>


            <LinearLayout
